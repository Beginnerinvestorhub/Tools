# Python Engine Environment Variables (FastAPI)
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/investment_hub
REDIS_URL=redis://localhost:6379

# CORS Configuration
CORS_ORIGINS=https://your-app.vercel.app,http://localhost:3000

# AI Services
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Internal Security
NUDGE_ENGINE_SECRET=your_internal_api_secret_key

# External APIs
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key
FINANCIAL_MODELING_PREP_API_KEY=your_fmp_key

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false
